    "questions": {
      "shippingTime": {
        "question": "How long does shipping take?",
        "answer": "Most orders ship within 1-2 business days and arrive in 3-5 business days. Express shipping options are available at checkout."
      },
      "paymentMethods": {
        "question": "What payment methods do you accept?",
        "answer": "We accept Visa, Mastercard, American Express, PayPal, Apple Pay, and Google Pay. All payments are securely processed."
      },
      "returns": {
        "question": "Can I return or exchange an item?",
        "answer": "Yes! We offer free returns within 30 days of purchase. Items must be unused and in original packaging. Visit our Returns Center to initiate a return."
      },
      "tracking": {
        "question": "How do I track my order?",
        "answer": "Once your order ships, you'll receive a tracking number via email. You can also check order status by logging into your account."
      },
      "internationalShipping": {
        "question": "Do you offer international shipping?",
        "answer": "Yes, we ship to over 100 countries worldwide. International shipping rates and delivery times vary by destination."
      },
      "createAccount": {
        "question": "How do I create an account?",
        "answer": "Click 'Sign Up' in the top navigation. You can also create an account during checkout by selecting 'Save my information for next time'."
      }
    },
